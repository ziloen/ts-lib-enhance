{
  "name": "typescript-lib-enhancer",
  "version": "1.0.0",
  "description": "Enhance typescript default lib.*.d.ts",
  "keywords": [
    "typescript",
    "types"
  ],
  "author": "ziloen",
  "main": "shim.d.ts",
  "types": "shim.d.ts",
  "sideEffects": false,
  "files": [
    "*.d.ts"
  ],
  "scripts": {
    "release": "bumpp package.json --commit --no-push && pnpm -r publish --access public"
  },
  "dependencies": {
    "@ziloen/eslint-config": "^0.0.65",
    "bumpp": "^8.2.1",
    "typed-query-selector": "^2.8.0",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@ziloen",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}