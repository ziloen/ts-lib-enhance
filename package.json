{
  "name": "typescript-lib-enhancer",
  "version": "1.0.6",
  "description": "Enhance typescript default lib.*.d.ts",
  "keywords": [
    "typescript",
    "types"
  ],
  "homepage": "https://github.com/ziloen/typescript-lib-enhancer",
  "author": "ziloen",
  "main": "shim.d.ts",
  "types": "shim.d.ts",
  "sideEffects": false,
  "files": [
    "lib.dom.d.ts",
    "lib.es.d.ts",
    "shim.d.ts",
    "utils.d.ts"
  ],
  "scripts": {
    "dev": "tsc --noEmit --watch",
    "release": "bumpp package.json --commit --no-push && pnpm -r publish --access public",
    "lint": "eslint . --fix",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "bumpp": "^8.2.1",
    "typed-query-selector": "^2.8.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@ziloen/eslint-config": "^0.0.78",
    "@ziloen/eslint-config-format": "^0.0.78",
    "eslint": "^8.28.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ziloen",
      "@ziloen/eslint-config-format"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "prefer-object-has-own": "off"
    }
  }
}
