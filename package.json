{
  "name": "ts-lib-enhance",
  "version": "1.0.15",
  "type": "module",
  "description": "Enhance typescript default lib.*.d.ts",
  "keywords": [
    "typescript",
    "types"
  ],
  "homepage": "https://github.com/ziloen/ts-lib-enhance",
  "author": "ziloen",
  "main": "shim.d.ts",
  "types": "shim.d.ts",
  "sideEffects": false,
  "files": [
    "lib.dom.d.ts",
    "lib.es.d.ts",
    "shim.d.ts",
    "utils.d.ts",
    "highlight.d.ts",
    "view-transition.d.ts",
    "InputEventInputType.d.ts",
